<svg id="svgCircle" xmlns="http://www.w3.org/2000/svg" 
     width="${param.radius*2+param.xPos}" 
     height="${param.radius*2+param.yPos}"  
     style="position: relative; left: 50px;
     background-color: #ffff00; " >
  <circle cx="${param.xPos}" 
          cy="${param.yPos}" 
          r="${param.radius}" 
          fill="${param.color}"  />
</svg>
